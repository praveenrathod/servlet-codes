<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>it-companies</display-name>

	<welcome-file-list>
		<welcome-file>order.html</welcome-file>

	</welcome-file-list>
	<servlet>
		<servlet-name>xboxServlet</servlet-name>
		<servlet-class>com.xworkz.box.XboxServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>xboxServlet</servlet-name>
		<url-pattern>/orderAction</url-pattern>
	</servlet-mapping>

</web-app>
